package ovn3;

public class Ovn3 {
	public static void main(String[] args) 
	{
		int sum1 = 0;
		for(int i = 1; i < 100; i++) {
			sum1 = sum1 + i;
			System.out.println(sum1);
		}
		int sum2 = 1;
		int antaltermer1 = 0;
		while(sum2 < 10000) {
			sum2 = sum2 + 2;
			antaltermer1 = antaltermer1 + 1;
		}
		System.out.println(antaltermer1);
	}
}
