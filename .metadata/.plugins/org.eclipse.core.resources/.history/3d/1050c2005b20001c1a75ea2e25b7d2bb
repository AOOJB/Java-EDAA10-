import java.util.Random;

public class HelloWorld {
  public static void main(String[] args) {
    System.out.println("Hello, world!");
  }
}
public class Die{
	private int dots;
	private Random rand;
	public die() {
		rand = new Random();
		dots = 1;
	}
	public void roll() {
		dots = rand.nextInt(6) + 1;
	}
}
